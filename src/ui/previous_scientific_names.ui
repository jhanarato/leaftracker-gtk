<?xml version='1.0' encoding='UTF-8'?>
<interface>
    <template class="PreviousScientificNames" parent="AdwPreferencesGroup">
        <property name="title">Previous Scientific Names</property>
        <property name="halign">fill</property>
        <property name="separate-rows">true</property>

        <child>
            <object class="AdwPreferencesRow">
                <child>
                    <object class="GtkListBox">
                        <property name="selection-mode">single</property>
                        <style>
                            <class name="boxed-list"/>
                        </style>
                        <child>
                            <object class="AdwActionRow">
                                <property name="title">Acacia one</property>
                            </object>
                        </child>
                        <child>
                            <object class="AdwActionRow">
                                <property name="title">Acacia two</property>
                            </object>
                        </child>
                        <child>
                            <object class="AdwActionRow">
                                <property name="title">Acacia three</property>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="AdwEntryRow" id="add_name_entry_row">
                <property name="title">Add Name</property>
            </object>
        </child>
    </template>
</interface>